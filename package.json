{
  "name": "front-end-starter",
  "homepage": ".",
  "sideEffects": false,
  "version": "1.0.0",
  "description": "InsuranceMenu internal starter-kit for FE projects.",
  "author": "Ivanovskyy Yegor",
  "private": true,
  "scripts": {
    "build": "npm run clean-dist && webpack --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --fix",
    "format": "prettier --write \"src/**/*.+(ts|tsx|json)\"",
    "start": "npm run start-dev",
    "start-dev": "webpack serve --config=configs/webpack/dev.js",
    "start-prod": "npm run build && node express.js",
    "test": "jest --config=configs/jest.json",
    "test-watch": "jest --coverage --watchAll --config=configs/jest.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-runtime": "^7.13.7",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^7.0.0",
    "core-js": "^3.9.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0",
    "husky": "^4.3.6",
    "image-webpack-loader": "^7.0.1",
    "imagemin-gifsicle": "6.0.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.18",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.11.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-jss": "^10.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not op_mini all",
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version",
      "IE 11",
      "last 2 edge version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "IE 11"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format && npm run test"
    }
  }
}
